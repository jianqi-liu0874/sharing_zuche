
.credit-wrapper {
	margin: 0 auto;
	width: 85%;
	border-bottom: 1rpx solid #E8E8E8;
}
.zhima-score-area {
	margin: 60rpx auto 40rpx;
	padding-top: 1rpx;
	text-align: center;
	width: 200rpx;
	height: 200rpx;
	background: url(data:image/png;base64,);
	background-size: 200rpx 200rpx;
	background-repeat: no-repeat;
}
.zhima-score {
	margin-top: 36rpx;
	font-size: 64rpx;
	font-weight: 600;
}
.zhima-score-text {
	margin-top: 10rpx;
	font-size: 28rpx;
}
.zhima-desc-area {
	margin: 60rpx auto;
	line-height: 60rpx;
	text-align: center;
}
.icon-zhengque,
.icon-cuowu {
	margin-right: 16rpx;
	font-size: 36rpx;
	color: #00CCCC;
}
.btn-wrapper {
	margin: 120rpx auto 0;
	width: 80%;
}
.btn-wrapper>button {
	color: #fff;
	border-color: #00CCCC;
	background-color: #00CCCC;
	border-radius: 12rpx;
}

