<view class="content"><view style="{{'background-color:'+(globalData.headColor)+';'}}"><view data-event-opts="{{[['tap',[['toLogin']]]]}}" class="top-wrapper" bindtap="__e"><image class="avatar" mode="scaleToFill" src="{{defaultAvatar}}"></image><block wx:if="{{hadLogined}}"><text class="phone">{{userData.phone}}</text></block><block wx:else><text class="login-text">点击登录</text></block></view><view class="main-wrapper"><block wx:if="{{hadLogined}}"><view class="main-info-area"><view data-event-opts="{{[['tap',[['toRecharge']]]]}}" class="main-info-item" style="{{'color:'+(globalData.commonTextColor1)+';'}}" bindtap="__e"><view class="main-info-text1"><text style="font-size:50rpx;">{{userData.balance}}</text><text>元</text></view><view class="main-info-text2">余额</view></view><view class="main-info-item" style="{{'color:'+(globalData.commonTextColor1)+';'}}"><view class="main-info-text1"><block wx:if="{{userData.depositState==1}}"><block><text style="font-size:50rpx;">{{userData.deposit}}</text><text>元</text></block></block><block wx:else><block wx:if="{{userData.depositState==20}}"><block><text>无押金</text></block></block><block wx:else><block wx:if="{{userData.depositState==100}}"><block><text>信用免押</text></block></block></block></block></view><view class="main-info-text2">押金</view></view></view></block><view class="main-list-area"><block wx:if="{{hadLogined}}"><view data-event-opts="{{[['tap',[['toMyOrders']]]]}}" class="main-list-item" bindtap="__e"><text class="iconfont icon-dingdan" style="{{'color:'+(globalData.mainColor)+';'}}"></text><text>我的订单</text><text class="iconfont icon-right arrow"></text></view></block><block wx:if="{{hadLogined}}"><view data-event-opts="{{[['tap',[['toConsumeDetail']]]]}}" class="main-list-item" bindtap="__e"><text class="iconfont icon-money" style="{{'color:'+(globalData.mainColor)+';'}}"></text><text>消费明细</text><text class="iconfont icon-right arrow"></text></view></block><block wx:if="{{hadLogined}}"><view data-event-opts="{{[['tap',[['toUseAgreement']]]]}}" class="main-list-item" bindtap="__e"><text class="iconfont icon-yonghu3" style="{{'color:'+(globalData.mainColor)+';'}}"></text><text>用户协议</text><text class="iconfont icon-right arrow"></text></view></block><block wx:if="{{hadLogined}}"><view data-event-opts="{{[['tap',[['toContract',['$event']]]]]}}" class="main-list-item" bindtap="__e"><text class="iconfont icon-yonghu3" style="{{'color:'+(globalData.mainColor)+';'}}"></text><text>我的签约</text><text class="iconfont icon-right arrow"></text></view></block><view class="main-list-item"><text class="iconfont icon-banbenhao" style="{{'color:'+(globalData.mainColor)+';'}}"></text><text>版本号</text><text style="margin-left:6rpx;">v1.3.4</text></view><block wx:if="{{hadLogined}}"><view data-event-opts="{{[['tap',[['logOut']]]]}}" class="main-list-item" bindtap="__e"><text class="iconfont icon-tuichu" style="{{'color:'+(globalData.mainColor)+';'}}"></text><text>退出登录</text></view></block></view></view></view></view>