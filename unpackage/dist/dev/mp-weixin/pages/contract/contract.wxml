<view class="content"><view class="tab-content"><block wx:for="{{papcList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item-wrapper" data-data="{{item}}" data-index="{{index}}"><view class="paid-item-top"><text style="{{'color:'+(globalData.mainColor)+';'}}">已签约</text></view><view class="paid-item-bottom"><view class="paid-item-bottom-item"><text>签约号</text><text class="paid-item-bottom-item-right">{{item.ktNo}}</text></view><view class="paid-item-bottom-item"><text>下次扣款时间</text><text class="paid-item-bottom-item-right">{{item.nextPayDate}}</text></view><view class="paid-item-bottom-item"><text>租赁周期</text><block wx:if="{{item.periodUnit==0}}"><text class="paid-item-bottom-item-right">按小时租</text></block><block wx:if="{{item.periodUnit==7}}"><text class="paid-item-bottom-item-right">按分钟租</text></block><block wx:if="{{item.periodUnit==1}}"><text class="paid-item-bottom-item-right">按天租</text></block><block wx:if="{{item.periodUnit==5}}"><text class="paid-item-bottom-item-right">按周租</text></block><block wx:if="{{item.periodUnit==2}}"><text class="paid-item-bottom-item-right">按月租</text></block><block wx:if="{{item.periodUnit==3}}"><text class="paid-item-bottom-item-right">按季租</text></block><block wx:if="{{item.periodUnit==4}}"><text class="paid-item-bottom-item-right">按年租</text></block><block wx:if="{{item.periodUnit==6}}"><text class="paid-item-bottom-item-right">按月租（以租代购）</text></block></view><view class="btn-wrapper"><view class="pay-btn" data-data="{{item.ktNo}}" data-event-opts="{{[['tap',[['toHistory',['$event']]]]]}}" bindtap="__e">查看扣款记录</view></view></view></view></block><view class="bottom-tips">没有更多记录了</view></view></view>