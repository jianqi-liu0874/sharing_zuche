
.content {
	position: relative;
}
.collapse-wrapper {
	padding-bottom: 160rpx;
}
.item-content {
	padding: 28rpx 32rpx;
	font-size: 34rpx;
	color: #333;
	line-height: 48rpx;
	background-color: #F5F5F5;
}
.bottom-wrapper {
	position: fixed;
	bottom: 0;
	left: 0;
	width: 100%;
}
.bottom-wrapper>button {
	display: inline-block;
	width: 50%;
	border-bottom: none;
}
.uni-collapse-cell__title {
	background-color: #fff;
}

